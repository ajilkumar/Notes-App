<div class="container-fluid container-fluid-custom pb-5 mb-5">

  <div class="row mb-4">
    <div class="col">
      <h1>Hey, <%= userName %></h1>
    </div>
    <div class="col text-end">
      <a href="/dashboard" class="btn btn-primary">+ New Note</a>
    </div>
  </div>


  <div class="row">

    <% if (notes.length > 0 ) { for(var i=0; i < notes.length; i++) { %>
     
      <div class="col-sm-3 mb-4">
        <div class="card border-primary" style="min-height: 210px">
        <a href="/dashboard/item<%= notes[i]._id %>"</a></div>
      </div>

    <% }} %>





  </div>





</div>